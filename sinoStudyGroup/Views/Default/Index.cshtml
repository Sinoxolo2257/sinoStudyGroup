


@{
    ViewBag.Title = "Index";
}

<h2>Study Group</h2>

<table class="table">
    <thead>
        <tr>
            <th> @Html.DisplayNameFor(model => model.StudentNo) </th>
            <th> @Html.DisplayNameFor(model => model.FirstName) </th>
            <th> @Html.DisplayNameFor(model => model.Surname) </th>
            <th> @Html.DisplayNameFor(model => model.Email) </th>
        </tr>
    </thead>



    <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td> @Html.DisplayTextFor(modelItem => item.StudentNo) </td>
                    <td> @Html.DisplayTextFor(modelItem => item.FirtName) </td>
                    <td> @Html.DisplayTextFor(modelItem => item.Surname) </td>
                    <td> @Html.DisplayTextFor(modelItem => item.Email) </td>
                </tr>
            }
        </tbody>
</table>